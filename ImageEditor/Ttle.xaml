<Window x:Class="ImageEditor.Ttle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageEditor"
        mc:Ignorable="d"
        Title="Ttle" Height="200" Width="350">
    <Grid Background="Black">
        <TextBox HorizontalAlignment="Left" Height="23" Margin="28,60,0,0" TextWrapping="Wrap" Name="v" VerticalAlignment="Top" Width="281" />
        <Button Content="OK" HorizontalAlignment="Left" Margin="105,114,0,0" VerticalAlignment="Top" Width="75" Click="btnok"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="215,114,0,0" VerticalAlignment="Top" Width="75" Click="btncancel"/>
        <Label Content="Add your Title" HorizontalAlignment="Left" Margin="107,15,0,0" VerticalAlignment="Top" Width="164" Foreground="White"/>

    </Grid>
</Window>
