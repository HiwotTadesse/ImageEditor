<Window x:Class="ImageEditor.insertImage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageEditor"
        mc:Ignorable="d"
        Title="insertImage" Height="200" Width="500">
    <Grid Margin="2,0,0,-2" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}">
        <TextBox HorizontalAlignment="Left" Height="23" Margin="126,26,0,0" TextWrapping="Wrap" Name="x"  VerticalAlignment="Top" Width="324"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="127,71,0,0" TextWrapping="Wrap" Name="y"  VerticalAlignment="Top" Width="324"/>
        <Button Content="OK" HorizontalAlignment="Left" Click="btnok"  Margin="130,136,0,0" VerticalAlignment="Top" Width="75" />
        <Button Content="Cancel" HorizontalAlignment="Left" Click="btncancel" Margin="224,136,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="X Position" HorizontalAlignment="Left" Margin="23,26,0,0" VerticalAlignment="Top" Width="91" Foreground="White" FontSize="18"/>
        <Label Content="Y Position" HorizontalAlignment="Left" Margin="25,71,0,0" VerticalAlignment="Top" Width="89" Foreground="White" FontSize="18"/>


    </Grid>
</Window>
